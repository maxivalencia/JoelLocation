{{ form_start(form) }}

<div class="row">
	<div class="form-group col-md-6">
		<label for="">Nom :</label>
		{{ form_widget(form.nom) }}
	</div>
	<div class="form-group col-md-6">
		<label for="">Prénom :</label>
		{{ form_widget(form.prenom) }}
	</div>

	<div class="form-group col-md-6">
		<label for="">Mot de passe :</label>
		{{ form_widget(form.password) }}

	</div>
	<div class="form-group col-md-6">
		<label for="">Adresse email :</label>
		{{ form_widget(form.mail) }}

	</div>
	<div class="form-group col-md-3">
		<label for="">Téléphone :</label>
		{{ form_widget(form.telephone) }}

	</div>
	<div class="form-group col-md-3">
		<label for="">Portable :</label>
		{{ form_widget(form.portable) }}

	</div>
	<div class="form-group col-md-3">
		<label for="">Date de Naissance :</label>
		{{ form_widget(form.dateNaissance) }}

	</div>
	<div class="form-group col-md-3">
		<label for="">Lieu de Naissance :</label>
		{{ form_widget(form.lieuNaissance) }}

	</div>
	<div class="form-group col-md-3">
		<label for="">Numero de permis :</label>
		{{ form_widget(form.numeroPermis) }}

	</div>
	<div class="form-group col-md-3">
		<label for="">Date permis :</label>
		{{ form_widget(form.datePermis) }}

	</div>
	<div class="form-group col-md-6">
		<label for="">Adresse :</label>
		{{ form_widget(form.adresse) }}

	</div>
	<div class="form-group col-md-6">
		<label for="">Rôles :</label>
		{{ form_widget(form.fonction) }}

	</div>
	<div class="form-group col-md-6">
		<label for="">Complement d'adresse :</label>
		{{ form_widget(form.complementAdresse) }}

	</div>
	<div class="form-group col-md-6">
		<label for="">Ville :</label>
		{{ form_widget(form.ville) }}

	</div>
</div>
<div class="">
	<button class="btn btn-danger">
		<i class="fa fa-book" aria-hidden="true"></i>
		{{ button_label|default('Enregistrer') }}
	</button>
</div>
{{ form_end(form) }}
